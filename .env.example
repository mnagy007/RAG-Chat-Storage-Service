# App
APP_PORT=8080
API_KEY=rag-chat-token
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173
SECURITY_ALLOWLIST=/actuator/health,/v3/api-docs,/swagger-ui,/swagger-ui.html

# Postgres
DB_NAME=rag
DB_USER=rag
DB_PASS=rag

# JDBC (app uses DB_URL; compose wires this automatically)
DB_URL=jdbc:postgresql://db:5432/rag

# Rate limit
RATE_LIMIT_CAPACITY=300
RATE_LIMIT_PERIOD_MINUTES=5

# pgAdmin (optional)
PGADMIN_EMAIL=admin@example.com
PGADMIN_PASSWORD=admin

# Swagger
SWAGGER_DOCS_ENABLED=true
SWAGGER_REQUEST_SERVER_URL=http://localhost:8080
SWAGGER_URL=http://localhost:8080/swagger-ui/index.html
